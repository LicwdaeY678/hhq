@import '../../font/iconfont.wxss';

.top-bg{
  width: 100%;
  background: #f42f19;
  text-align: center;
  height: 120rpx;
  color:#fff;
  border-bottom-right-radius: 50%;
  border-bottom-left-radius: 50%;
  position: relative;
}
.circle{
  position: absolute;
  left:50%;
  bottom: 0;
  transform: translate(-50%,50%);
  color: #fff;
  width: 130rpx;
  height:130rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
}



.contact{
  width: 50px;
  height:50px;
  position: fixed;
  right: 10px;
  bottom: 20%;
  font-size: 24px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius:25px;
  padding:0;
  margin:0;
  background: #f1f1f1;
  border: 1rpx solid #f1f1f1;
  line-height: 30px;
  z-index: 99;
}
.contact text{
  font-size: 24px;
}




