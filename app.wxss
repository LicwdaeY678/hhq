/**app.wxss**/
@import './font/iconfont.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

button::after {
  border: none;
}

button {
  margin: 0;
  line-height: 1;
  padding: 0;
  text-align: left;
  background: transparent;
}
